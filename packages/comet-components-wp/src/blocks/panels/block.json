{
	"name": "comet/panels",
	"title": "Panels",
	"acf": {
		"mode": "preview",
		"renderTemplate": "index.php"
	},
	"allowedBlocks": ["comet/panel"],
	"category": "grouping",
	"icon": "list-view",
	"description": "Group related content into expandable/collapsible sections or a tabbed interface",
	"keywords": ["accordion", "tabs", "panels", "container", "wrapper", "section", "group"],
	"textdomain": "comet",
	"attributes": {
		"tagName": {
			"type": "string",
			"default": "div"
		},
		"variant": {
			"type": "string",
			"default": "comet/accordion"
		}
	},
	"variations": [
		{
			"name": "comet/accordion",
			"title": "Accordion",
			"icon": "list-view",
			"description": "Group related content in an expandable/collapsible interface",
			"scope": ["inserter", "transform"],
			"isDefault": true,
			"isActive": ["variant", "accordion"],
			"attributes": {
				"variant": "accordion"
			}
		},
		{
			"name": "comet/tabs",
			"title": "Tabs",
			"icon": "category",
			"description": "Group related content in a tabbed interface",
			"scope": ["inserter", "transform"],
			"isActive": ["variant", "tabs"],
			"attributes": {
				"variant": "tabs"
			}
		}
	],
	"supports": {
		"mode": false,
		"jsx": true,
		"innerBlocks": true,
		"anchor": true,
		"ariaLabel": true,
		"align": false,
		"html": false,
		"color": {
			"gradients": true,
			"background": true,
			"text": false,
			"__experimentalDefaultControls": {
				"background": true
			}
		},
		"layout": {
			"allowEditing": true,
			"allowOrientation": true
		}
	}
}
