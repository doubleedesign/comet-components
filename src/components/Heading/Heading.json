{
    "name": "Heading",
    "content": {
        "type": "string",
        "required": true
    },
    "attributes": {
        "tag": {
            "type": "HeadingTag",
            "supported": [
                "h1",
                "h2",
                "h3",
                "h4",
                "h5",
                "h6"
            ],
            "default": "h2"
        },
        "id": {
            "type": "string",
            "description": "Unique identifier for the component instance"
        },
        "style": {
            "type": "string",
            "description": "Inline styles to be applied to the component HTML"
        },
        "textAlign": {
            "type": "Alignment",
            "supported": [
                "left",
                "right",
                "center",
                "justify",
                "start",
                "end",
                "match-parent"
            ],
            "default": "start"
        },
        "classes": {
            "type": "array<string>",
            "description": "CSS classes to be applied to the component HTML",
            "supported": [
                "is-style-accent",
                "is-style-lead"
            ],
            "default": []
        }
    }
}