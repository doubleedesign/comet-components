{
	"name": "doubleedesign/comet-components",
	"type": "library",
	"authors": [
		{
			"name": "Leesa Ward",
			"email": "leesa@doubleedesign.com.au"
		}
	],
	"autoload": {
		"classmap": [
			"src/components/"
		],
		"psr-4": {
			"Doubleedesign\\Comet\\Components\\": [
				"src/base/types/",
				"src/base/traits/",
				"src/base/components/",
				"src/base/"
			],
			"Doubleedesign\\Comet\\Services\\": "src/services/",
			"Doubleedesign\\Comet\\Tests\\": "test/unit/",
			"Doubleedesign\\Comet\\TestUtils\\": "test/common/",
			"Doubleedesign\\Comet\\TestUtils\\WordPress\\": "test/common/wordpress/"
		}
	},
	"require-dev": {
		"brain/monkey": "^2.6.2",
		"phpunit/phpunit": "9.6.22",
		"roots/wordpress": "^6.7"
	},
	"config": {
		"allow-plugins": {
			"roots/wordpress-core-installer": true
		}
	},
	"require": {
		"illuminate/view": "^10.0",
		"illuminate/filesystem": "^10.0",
		"illuminate/events": "^10.0",
		"illuminate/container": "^10.0",
		"illuminate/support": "^10.0",
		"ezyang/htmlpurifier": "^4.18"
	}
}
